@model IEnumerable<TECNOLIGICCASE.Models.HistorialEdicionesCargo>

<h1>Historial de Ediciones de Cargos</h1>
<br>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Fecha de Edición</th>
            <th>Cargo</th>
            <th>Datos Antiguos</th>
            <th>Datos Nuevos</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FechaEdicion.ToString("dd/MM/yyyy HH:mm:ss")</td>
                <td>@item.Descripcion</td>
                <td>@Html.Raw(item.DatosAntiguos)</td>
                <td>@Html.Raw(item.DatosNuevos)</td>
            </tr>
        }
    </tbody>
</table>
